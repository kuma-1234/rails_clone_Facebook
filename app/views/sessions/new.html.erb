<h1>Log in</h1>
  <%= form_with(scope: :session, url: sessions_path, local: true ) do |f| %>

<form>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div class="form-group">
  <%= f.label :password %>
  <%= f.password_field :password %>
  </div>

  <%= f.submit"Log in"%> 
</form>
<% end %>

